  <connectionStrings/>
  <system.web>
    <httpModules>
      <add type="UniqueStudio.HttpControlLayer.HttpUserAuthorization,UniqueStudio.HttpControlLayer" name="HttpUserAuthorization"/>
      <!--<add type="UniqueStudio.HttpControlLayer.UrlRewriter,UniqueStudio.HttpControlLayer" name="UrlRewriter"/>-->
    </httpModules>
  </system.web>
</configuration>
